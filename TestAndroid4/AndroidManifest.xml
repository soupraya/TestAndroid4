<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.velan.testandroid4">

    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="19"/>

    <uses-permission android:name="android.permission.INTERNET"/>
   <permission android:name="com.velan.testandroid4.permission.C2D_MESSAGE" android:protectionLevel="signature"/>

   <uses-permission android:name="com.velan.testandroid4.permission.C2D_MESSAGE"/>
   <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE"/>
   <uses-permission android:name="android.permission.WAKE_LOCK"/>
   <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
   <uses-permission android:name="android.permission.USE_CREDENTIALS"/>
   <application android:name="com.velan.testandroid4.model.PricingApplication"
       android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">
       
        <activity android:label="@string/app_name" android:name="com.velan.testandroid4.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        
        <meta-data android:name="com.google.android.gms.version" android:value="@integer/google_play_services_version"/>
        <service android:name="com.velan.testandroid4.model.GCMIntentService"/>
        <receiver android:name="com.google.android.gcm.GCMBroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="com.velan.testandroid4"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="com.velan.testandroid4"/>
            </intent-filter>
        </receiver>
 
        <activity android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme" android:parentActivityName=".MainActivity" android:name="com.velan.testandroid4.ui.NavigationDrawerActivity"/>
</application>

</manifest>